my_dog1 = Dog("Rex", 5)
my_dog1.bark()           
print(my_dog1.get_age()) 
my_dog1.set_age(6)
print(my_dog1.get_age()) 

my_dog2 = Dog("Odi", 9)
my_dog2.bark()           
print(my_dog2.get_age()) 
my_dog2.set_age(6)
print(my_dog2.get_age())


my_cat1 = Cat("Whiskers", 3)
my_cat1.meow()            
print(my_cat1.get_age()) 
my_cat1.set_age(4)
print(my_cat1.get_age())

my_cat2 = Cat("Garfild", 9)
my_cat2.meow()            
print(my_cat2.get_age()) 
